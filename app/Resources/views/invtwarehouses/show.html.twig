{% extends 'base.html.twig' %}

{% block body %}
    <h1>InvtWarehouses</h1>

    <table>
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ invtWarehouse.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ invtWarehouse.name }}</td>
            </tr>
            <tr>
                <th>Desc</th>
                <td>{{ invtWarehouse.desc }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ invtWarehouse.zipcode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ invtWarehouse.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ invtWarehouse.country }}</td>
            </tr>
            <tr>
                <th>Posttolocation</th>
                <td>{{ invtWarehouse.postToLocation }}</td>
            </tr>
            <tr>
                <th>Deletedat</th>
                <td>{% if invtWarehouse.deletedat %}{{ invtWarehouse.deletedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ invtWarehouse.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('invtwarehouses_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('invtwarehouses_edit', { 'id': invtWarehouse.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
